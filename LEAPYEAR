#include<stdio.h>
void main()
{
    int a;
    printf("Enter the Year:");
    scanf("%d",&a);
    if(a%4==0)
    {
        if(a%100==0)
            {
            if(a%400==0)
                printf("it is leap year");
            else
                printf("it is not leap year");
            }
        else{
            printf("it is leap year");
        }
        }
        else
            printf("it is not a leap year");
}
